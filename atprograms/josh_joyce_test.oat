int[] arr1 = {1,2,3,4};
int[] arr2 = {1,2,3,5};

int arrcheck(int[] ar1, int[] ar2, int len){
	int val = 0;
	for(int i =0; i < len; i= i+1;){
		if (ar1[i] != ar2[i]) {
			val = 1;
		}
	}
	return val;
}

int program(int argc, string[] argv) {

  	int val = 1;
    if(arrcheck(arr1, arr2, 4) == 1) {val = 0;}
    return val;

}
